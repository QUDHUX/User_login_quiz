<!DOCTYPE html>
<html>
  <%- include('./partials/header.ejs')%>
<body>
    <div class="quizContainer container-fluid well well-lg">
      <%- include('./partials/nav.ejs')%>
        <div id="quiz1" class="text-center">
			<h3>Quran Memorization School 2022</h3>
			<center><img class="img-responsive" height="180" width="180" src="http://res.cloudinary.com/dwjej2tbp/image/upload/v1523002021/KGCAS_SK_eyehy9.jpg"></center>
			<h4 style="color:#FF0000;position:absolute;left:70%;top:30%;" align="center" ><span id="iTimeShow"></span><br/><span id='timer' style="font-size:25px;"></span></h4>
			<h2>QMS Assessment</h2>
          <div class="row justify-content-center">
            <div class="card" style="width: 30rem;">
                <div class="card-body">
                  <h5 class="card-title">Login Form</h5>
                   <form action="/auth/login" method="POST">
                    <div class="form-group">
                      <label for="email">Email address</label>
                      <input type="email" class="form-control" name="email" id="email" aria-describedby="emailHelp" placeholder="Enter email">
                    </div>
                    <div class="form-group">
                      <label for="password">Password</label>
                      <input type="password" class="form-control" name="password" id="password" placeholder="Password">
                    </div>
                    <button type="submit" class="btn btn-primary">Login</button>
                  </form>
                  <% if(message){%>
                    <h4 class="alert alert-danger mt-4"><%= message %></h4>
                  <%}%>
                  
                </div>
              </div>
		</div>
  </div>
    </div>
    <script src="js/register.js"></script>
    <%- include('./partials/footer.ejs')%>
</body>
  </html>